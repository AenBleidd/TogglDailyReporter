<Window x:Class="TogglDailyReporter.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TogglDailyReporter"
        xmlns:View="clr-namespace:TogglDailyReporter"
        xmlns:ViewModel="clr-namespace:TogglDailyReporter"
        mc:Ignorable="d"
        Title="Toggl Daily Reporter" 
        Height="500" 
        Width="720"
        WindowStartupLocation="CenterScreen"
        WindowState="Maximized"
        >
  <Window.DataContext>
    <ViewModel:ReportViewModel x:Name="vmReportViewModel"/>
  </Window.DataContext>
  <View:ReportView x:Name="vReportView"/>
</Window>
